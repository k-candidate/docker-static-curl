{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["curl-version.txt"],
      "matchStrings": ["^(?<currentValue>\\d+\\.\\d+\\.\\d+)$"],
      "datasource": "github-releases",
      "depName": "curl/curl",
      "versioning": "semver"
    }
  ]
}
