{
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["curl-version.txt"],
      "matchStrings": ["^(?<currentValue>curl-(?<major>\\d+)_(?<minor>\\d+)_(?<patch>\\d+)$)"],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "curl/curl",
      "versioningTemplate": "regex"
    }
  ]
}
